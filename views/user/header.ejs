<div class="navbar">

    <div class="row return_row" >

      <div class="col-1 no-padding ">
          <img src = "./images/new_logo.png" class=" margin-top-7_5 logo" alt = "logo" class="logo"/>
      </div>

      <div class="col-2  no-padding margin-top-2_5">
