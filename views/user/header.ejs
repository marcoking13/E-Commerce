          </div>
      </div>
      <% console.log(isAuthenticated) %>
      <% if (! isAuthenticated){ %>
      <div class="col-2  no-padding margin-top-2_5">
          <p class="return_to "style="padding-left:10%">Hello! Sign in to <br /><span style="font-weight:bold"> <a href="/login">Customer</a> |  Admin </span></p>
      </div>
      <% }else{ %>
        <div class="col-2  no-padding margin-top-2_5">
            <p class="return_to"style="font-size:16px;padding-left:10%">Welcome Back!<br />
              <a href = "/logout" >
